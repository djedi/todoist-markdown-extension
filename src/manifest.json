{
  "manifest_version": 2,
  "name": "Todoist Markdown Export",
  "version": "1.0",
  "description": "Export Todo List to Markdown Text to your clipboard",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://todoist.com/app/*"],
      "js": ["content.js"]
    }
  ]
}
